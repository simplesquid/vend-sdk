{
    "name": "simplesquid/vend-sdk",
    "description": "An unofficial PHP SDK for Lightspeed Retail (X-Series)",
    "keywords": [
        "simplesquid",
        "vend-sdk",
        "sdk",
        "point-of-sale",
        "vend",
        "lightspeed"
    ],
    "homepage": "https://github.com/simplesquid/vend-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Poulter",
            "email": "matthew.poulter@simplesquid.co.za",
            "homepage": "https://simplesquid.co.za/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "sammyjo20/saloon": "v2.x-dev"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.9",
        "spatie/ray": "^1.28",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleSquid\\Vend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSquid\\Vend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan analyse",
        "lint": "./vendor/bin/pint",
        "test": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
