{
    "name": "simplesquid/vend-sdk",
    "description": "An unofficial PHP SDK for Lightspeed Retail (X-Series)",
    "keywords": [
        "simplesquid",
        "vend-sdk",
        "sdk",
        "point-of-sale",
        "vend",
        "lightspeed"
    ],
    "homepage": "https://github.com/simplesquid/vend-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Poulter",
            "email": "matthew.poulter@simplesquid.co.za",
            "homepage": "https://simplesquid.co.za/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleSquid\\Vend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSquid\\Vend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
